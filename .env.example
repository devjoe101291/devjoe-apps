# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase Project URL
VITE_SUPABASE_URL=https://your-project-ref.supabase.co

# Your Supabase Anon/Public Key
VITE_SUPABASE_PUBLISHABLE_KEY=your-anon-key-here

# Cloudflare R2 Configuration (for large file uploads > 50MB)
VITE_R2_ACCESS_KEY_ID=your-r2-access-key-id
VITE_R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
VITE_R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
VITE_R2_BUCKET_NAME=your-bucket-name
VITE_R2_PUBLIC_URL=https://pub-xxxxx.r2.dev
